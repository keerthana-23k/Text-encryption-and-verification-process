<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.crypto_app">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Crypto_App">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SignatureActivity">
            <intent-filter>
                <action android:name="com.example.cryptographyapp.SignatureActivity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".EncryptionActivity">
            <intent-filter>
                <action android:name="com.example.cryptographyapp.EncryptionActivity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>

        </activity>
        <activity android:name=".MainActivity2" >
            <intent-filter>
                <action android:name="com.example.cryptographyapp.MainActivity2" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity1">
            <intent-filter>
                <action android:name="com.example.cryptographyapp.MainActivity1" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>

        </activity>


    </application>

</manifest>